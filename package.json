{
  "name": "gut-flow",
  "version": "1.0.0",
  "description": "Ein gut git Flow",
  "preferGlobal": true,
  "bin": {
    "gut": "index.js"
  },
  "scripts": {
    "lint": "eslint '*/**' 'index.js'",
    "test": "node test/cleanup.js; npm run lint; jest",
    "install-prod": "npm install --save --save-exact",
    "install-dev": "npm install --save-dev --save-exact",
    "ncu": "npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quilicicf/Gut.git"
  },
  "keywords": [
    "git",
    "flow"
  ],
  "author": "quilicicf <quilicicf@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/quilicicf/Gut/issues"
  },
  "homepage": "https://github.com/quilicicf/Gut#readme",
  "dependencies": {
    "base-64": "0.1.0",
    "colors": "1.2.3",
    "copy-paste": "1.3.0",
    "inquirer": "5.2.0",
    "lodash": "4.17.10",
    "utf8": "3.0.0",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jest": "21.15.1",
    "fs-extra": "6.0.0",
    "jest": "22.4.3"
  }
